<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>ReversiGame.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
  <servlet-name>reversiImplementation</servlet-name>
  <servlet-class>com.my.project.server.ReversiImplementation</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>reversiImplementation</servlet-name>
  <url-pattern>/com.my.project.ReversiGame/Reversi</url-pattern>
</servlet-mapping>

</web-app>
